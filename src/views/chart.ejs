  <body class="chart_sect">
    <%- include('partials/navigation') %>

    <div class="container p-4">
      <div class="col-md-4 offset-md-4">
        <h1>Chart</h1>
        <div>
          <canvas id="myChart"></canvas>
        </div>
      </div>
    </div>
    <script src="https://cdn.jsdelivr.net/npm/chart.js"></script>
    <script>
      let ctx = document.getElementById("myChart").getContext("2d");
      const labels = [
        "2012",
        "2013",
        "2014",
        "2015",
        "2016",
        "2017",
        "2018",
        "2019",
        "2020",
      ];

      const data = {
        labels,
        datasets: [
          {
            label: "Dataset 1",
            data: [10, 11, 9.9, 11.2, 10.5, 9.8],
            label: "time set",
          },
          {
            label: "Dataset 2",
            data: [10, 11, 9.9, 11.2, 10.5, 9.8],
            label: "time set",
          },
        ],
      };

      const config = {
        type: "line",
        data: data,
        options: {
          responsive: true,
          interaction: {
            mode: "index",
            intersect: false,
          },
          stacked: false,
          plugins: {
            title: {
              display: true,
              text: "Chart.js Line Chart - Multi Axis",
            },
          },
          scales: {
            y: {
              type: "linear",
              display: true,
              position: "left",
            },
            y1: {
              type: "linear",
              display: true,
              position: "right",

              // grid line settings
              grid: {
                drawOnChartArea: false, // only want the grid lines for one axis to show up
              },
            },
          },
        },
      };

      const myChart = new Chart(ctx, config);

      /*new Chart(ctx, {
